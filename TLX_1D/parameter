#include <bits/stdc++.h>
using namespace std;

void angka(int a){

		switch(a) {
			case 1 ... 9:
				cout<<"satuan"<<endl;
				break;
			case 10 ... 99:
				cout<<"puluhan"<<endl;
				break;
			case 100 ... 999:
				cout<<"ratusan"<<endl;
				break;
			case 1000 ... 9999:
				cout<<"ribuan"<<endl;
				break;
			case 10000 ... 99999:
				cout<<"puluhribuan"<<endl;
				break;}
}
int main()
{
	int a;
	while(cin>>a)
	{
		angka(a);
	}
}
